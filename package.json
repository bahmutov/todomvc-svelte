{
  "name": "todomvc-svelte",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blacksonic/todomvc-svelte.git"
  },
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write 'src/**/*.{js,svelte}'",
    "lint": "eslint 'src/**/*.{js,svelte}'",
    "start": "rollup -c -w",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest src",
    "test:e2e": "start-server-and-test start http://localhost:9500 cypress",
    "cypress": "cypress run"
  },
  "dependencies": {
    "svelte": "^3.20.1",
    "todomvc-app-css": "^2.3.0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.2",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/svelte": "^3.0.0",
    "babel-jest": "^25.3.0",
    "cypress": "^4.5.0",
    "cypress-svelte-unit-test": "3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "jest": "^25.3.0",
    "jest-extended": "^0.11.5",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^2.6.0",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-postcss": "^2.8.2",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "start-server-and-test": "^1.11.0",
    "svelte-jester": "^1.0.5"
  },
  "engines": {
    "node": "12"
  }
}
